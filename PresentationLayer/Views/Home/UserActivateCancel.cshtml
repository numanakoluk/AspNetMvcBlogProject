@using EntiyLayers.Messages
@model List<ErrorMessageObj>

@{
    ViewBag.Title = "Geçersiz İşlem";
}
<link href="~/Content/Login-register.css" rel="stylesheet" />

<section class="login-info">
    <div class="container">
        <div class="row main">
            <div class="form-header">
                <h1 class="text-center ">Yönlendiriliyorsunuz </h1>
            </div>
            <div class="main-content">
                <div class="text-danger">
                    <h2><span class="glyphicon glyphicon-remove"></span>&nbsp;&nbsp;&nbsp;Geçersiz İşlem</h2>
                    <hr />
                    <p class="text-center">
                        @foreach (ErrorMessageObj err in Model)
                        {
                            @err.Message<br>
                        }

                    </p>
                </div>
            </div>
        </div>
    </div>
</section>





